    "def isPalindrome(x): \n",
    "    rev = ''.join(reversed(x))\n",
    "    if (x == rev): \n",
    "        print('YES')\n",
    "    else:\n",
    "        print('NO')\n",
    "\n",
    "\n",
    "t = int(input())\n",
    "for _ in range(t):\n",
    "    s = input()\n",
    "    s = ''.join(a.lower() for a in s if a.isalnum())\n",
    "    isPalindrome(s)"
   ]
